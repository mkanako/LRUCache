{
  "name": "@mkanako/lrucache",
  "version": "1.0.0",
  "description": "LRU Cache",
  "author": "kanako",
  "license": "MIT",
  "keywords": [
    "LRU",
    "Cache"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mkanako/LRUCache.git"
  },
  "bugs": "https://github.com/mkanako/LRUCache/issues/new",
  "homepage": "https://github.com/mkanako/LRUCache",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "main":"(pgrep -fq 'tsc.*-watch' || npm run compile) && node ./dist/index.js",
    "pub": "rm -rf ./dist && npm run compile && npm publish --access public"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.0.1",
    "typescript": "^3.9.7"
  }
}
